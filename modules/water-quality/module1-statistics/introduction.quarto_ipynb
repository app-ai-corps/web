   "source": [
    "# Let's visualize the pH scale\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "ph_scale = list(range(0, 15))\n",
    "colors = ['red']*3 + ['orange']*3 + ['yellow', 'lightgreen'] + ['green']*3 + ['lightgreen', 'yellow'] + ['orange', 'red']\n",
    "\n",
    "plt.figure(figsize=(12, 3))\n",
    "plt.bar(ph_scale, [1]*15, color=colors, edgecolor='black', width=1)\n",
    "plt.xlabel('pH Value', fontsize=12)\n",
    "plt.title('pH Scale: Where Is Water Safe?', fontsize=14)\n",
    "plt.xticks(range(0, 15))\n",
    "plt.yticks([])\n",
    "\n",
    "# Mark safe drinking water range\n",
    "plt.axvline(x=6.5, color='blue', linewidth=3, linestyle='--', label='Safe range')\n",
    "plt.axvline(x=8.5, color='blue', linewidth=3, linestyle='--')\n",
    "plt.legend(fontsize=12)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(\"Safe drinking water: pH between 6.5 and 8.5 (the blue zone)\")"
   ]
