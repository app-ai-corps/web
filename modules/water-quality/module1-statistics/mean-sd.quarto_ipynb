   "source": [
    "import numpy as np\n",
    "\n",
    "# Same pH readings\n",
    "ph_readings = [7.2, 7.1, 7.3, 7.0, 7.2, 7.3, 7.1]\n",
    "\n",
    "# Calculate mean the easy way!\n",
    "mean_ph = np.mean(ph_readings)\n",
    "\n",
    "print(\"pH readings:\", ph_readings)\n",
    "print(f\"Mean pH: {mean_ph:.2f}\")\n",
    "print(\"\\nThat was much easier! üòä\")"
   ]
